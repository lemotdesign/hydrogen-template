{
  "name": "hydrogen-storefront",
  "private": true,
  "sideEffects": false,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "build": "remix build",
    "dev": "remix build && ([ -n \"$NETLIFY_DEV\" ] && tail -f /dev/null || npx --yes netlify-cli@latest dev --offline)",
    "preview": "netlify serve",
    "lint": "eslint --no-error-on-unmatched-pattern --ext .js,.ts,.jsx,.tsx .",
    "typecheck": "tsc --noEmit",
    "codegen": "shopify hydrogen codegen"
  },
  "prettier": "@shopify/prettier-config",
  "dependencies": {
    "@netlify/edge-functions": "^2.11.0",
    "@netlify/remix-edge-adapter": "^3.4.0",
    "@netlify/remix-runtime": "^2.3.0",
    "@remix-run/react": "^2.17.0",
    "@shopify/cli": "^3.60.0",
    "@shopify/cli-hydrogen": "^7.0.0",
    "@shopify/hydrogen": "^2024.7.0",
    "graphql": "^16.8.0",
    "graphql-tag": "^2.12.6",
    "isbot": "^4.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@remix-run/dev": "^2.17.0",
    "@remix-run/eslint-config": "^2.17.0",
    "@shopify/prettier-config": "^1.1.2",
    "@total-typescript/ts-reset": "^0.5.0",
    "@types/eslint": "^8.56.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "eslint": "^8.57.0",
    "eslint-plugin-hydrogen": "^0.12.2",
    "prettier": "^3.0.0",
    "typescript": "^5.4.0"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "overrides": {
    "body-parser": "^1.20.3",
    "cross-spawn": "^7.0.6",
    "estree-util-value-to-estree": "^3.3.3",
    "form-data": "^4.0.4",
    "js-yaml": "^4.1.0",
    "undici": "^6.0.0"
  }
}
